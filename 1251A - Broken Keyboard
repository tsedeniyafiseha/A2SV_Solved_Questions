t = int(input())

for _ in range(t):
    s = input().strip()
    n = len(s)

    i = 0
    broken = set()

    while i < n:
        j = i
        while j < n and s[j] == s[i]:
            j += 1

        if (j - i) % 2 == 1:
            broken.add(s[i])

        i = j

    print(''.join(sorted(broken)))
